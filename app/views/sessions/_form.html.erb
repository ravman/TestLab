<%= semantic_form_for session, builder: ActiveAdmin::FormBuilder do |f| %>
  <%= f.inputs do %>
    <%= f.input :email %>
    <%= f.input :password %>
  <% end %>
  <%= f.actions class: :buttons do %>
    <%= f.action :submit, label: 'Sign in' %>
  <% end %>
  <%= link_to 'Forgot Password?', new_password_path %>
<% end %>
