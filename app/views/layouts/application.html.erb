<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <% if content_for?(:title) %>
      <title><%= content_for(:title) %> | Limb Lab</title>
    <% else %>
      <title>Limb Lab</title>
    <% end %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>
  <body class="active_admin logged_out">
    <div id="wrapper">
      <div id="content_wrapper">
        <%= render 'flashes' %>
        <div id="active_admin_content">
          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
